@author Prabdeep Singh pannu, Dii-->
<?import javafx.scene.layout.StackPane?>
<StackPane fx:id="rootPane" prefHeight="800.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.SplashScreenController">
