<BorderPane fx:id="root" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.AppointmentController">

    <center>
        <VBox prefHeight="500.0" prefWidth="800.0" styleClass="content">
            <children>

                <Label alignment="CENTER" prefHeight="42.0" prefWidth="794.0" styleClass="heading" text="Your TimeSlots" />
                <AnchorPane id="AnchorPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="150" prefWidth="800" xmlns:fx="http://javafx.com/fxml">
                    <children>
                        <TableView fx:id="tableView" layoutX="-4.0" prefHeight="487.0" prefWidth="800.0">
                            <columns>
                                <TableColumn fx:id="Teacher" prefWidth="170.0" text="Professor" />
