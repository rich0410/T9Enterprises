<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import java.net.URL?>
<BorderPane prefHeight="600.0" prefWidth="800.0" fx:id="root" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="UI.StudentTimeTable">

    <center>
        <VBox prefHeight="500.0" prefWidth="800.0" styleClass="content">
            <children>
                <Label styleClass="heading" text="Your TimeTable"/>

                <AnchorPane id="AnchorPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                            minWidth="-Infinity" prefHeight="500" prefWidth="800" xmlns:fx="http://javafx.com/fxml">
                    <children>
                        <TableView fx:id="tableView1" layoutX="-4.0" prefHeight="500.0" prefWidth="641.0">
                            <columns>
                                <TableColumn fx:id="CourseCodeID" prefWidth="220.0" text="CourseCodeID"/>
                                <TableColumn fx:id="Day" prefWidth="100.0" text="Day"/>
                                <TableColumn fx:id="Time" prefWidth="100.0" text="Time"/>
                                <TableColumn fx:id="Professor" prefWidth="100.0" text="Professor"/>
                                <TableColumn fx:id="Room" prefWidth="120.0" text="Room Number"/>
                            </columns>
                        </TableView>
                    </children>
                </AnchorPane>
            </children>
            <!--<Button contentDisplay="RIGHT" defaultButton="true" mnemonicParsing="false"-->
            <!--onAction="#handleDeleteAction" prefHeight="43.0" prefWidth="101.0" text="Delete"/>-->
        </VBox>
    </center>
    <bottom>
        <HBox styleClass="footer">
            <children>
                <Label text="Copyright T9-Enterprises"/>
            </children>
        </HBox>
    </bottom>

    <stylesheets>
        <URL value="@toolbar-demo.css"/>
    </stylesheets>
</BorderPane>
